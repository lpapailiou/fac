<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="800.0" prefWidth="800.0" maxHeight="Infinity" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.AppController" AnchorPane.topAnchor="0.0"
            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
    <children>
        <BorderPane maxHeight="Infinity" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0"
                    AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <right>
                <VBox prefHeight="Infinity" prefWidth="200.0" minWidth="200.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="start" mnemonicParsing="false" text="go" alignment="CENTER"/>
                    </children>
                </VBox>
            </right>
            <center>
                <VBox BorderPane.alignment="CENTER" maxHeight="Infinity" AnchorPane.topAnchor="0.0"
                      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">

                    <children>
                        <SplitPane dividerPositions="0.5" orientation="HORIZONTAL" prefHeight="2000.0"
                                   maxHeight="Infinity" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0"
                                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                            <items>
                                <AnchorPane AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0"
                                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                                    <children>
                                        <TextArea fx:id="input" layoutX="-1.0" layoutY="-1.0" AnchorPane.topAnchor="0.0"
                                                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                  AnchorPane.rightAnchor="0.0" text="number one = 1; print(one);"/>
                                    </children>
                                </AnchorPane>
                                <AnchorPane AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0"
                                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                                    <children>
                                        <TabPane fx:id="tabPane" layoutY="-2.0" AnchorPane.topAnchor="0.0"
                                                 AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                 AnchorPane.rightAnchor="0.0" tabClosingPolicy="UNAVAILABLE">
                                            <tabs>
                                                <Tab text="scanned tokens">
                                                    <content>
                                                        <AnchorPane AnchorPane.topAnchor="0.0"
                                                                    AnchorPane.bottomAnchor="0.0"
                                                                    AnchorPane.leftAnchor="0.0"
                                                                    AnchorPane.rightAnchor="0.0">
                                                            <children>
                                                                <TextArea fx:id="scanOut" disable="true" layoutY="-2.0"
                                                                          AnchorPane.topAnchor="0.0"
                                                                          AnchorPane.bottomAnchor="0.0"
                                                                          AnchorPane.leftAnchor="0.0"
                                                                          AnchorPane.rightAnchor="0.0"/>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </Tab>
                                                <Tab text="parse tree">
                                                    <content>
                                                        <AnchorPane AnchorPane.topAnchor="0.0"
                                                                    AnchorPane.bottomAnchor="0.0"
                                                                    AnchorPane.leftAnchor="0.0"
                                                                    AnchorPane.rightAnchor="0.0">
                                                            <children>
                                                                <TextArea fx:id="parseTreeOut" disable="true"
                                                                          layoutY="-2.0" AnchorPane.topAnchor="0.0"
                                                                          AnchorPane.bottomAnchor="0.0"
                                                                          AnchorPane.leftAnchor="0.0"
                                                                          AnchorPane.rightAnchor="0.0"/>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </Tab>
                                                <Tab text="parsed code">
                                                    <content>
                                                        <AnchorPane AnchorPane.topAnchor="0.0"
                                                                    AnchorPane.bottomAnchor="0.0"
                                                                    AnchorPane.leftAnchor="0.0"
                                                                    AnchorPane.rightAnchor="0.0">
                                                            <children>
                                                                <TextArea fx:id="codeOut" disable="true" layoutY="-2.0"
                                                                          AnchorPane.topAnchor="0.0"
                                                                          AnchorPane.bottomAnchor="0.0"
                                                                          AnchorPane.leftAnchor="0.0"
                                                                          AnchorPane.rightAnchor="0.0"/>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </Tab>
                                                <Tab text="execution result">
                                                    <content>
                                                        <AnchorPane AnchorPane.topAnchor="0.0"
                                                                    AnchorPane.bottomAnchor="0.0"
                                                                    AnchorPane.leftAnchor="0.0"
                                                                    AnchorPane.rightAnchor="0.0">
                                                            <children>
                                                                <TextArea fx:id="executeOut" disable="true"
                                                                          layoutY="-2.0" AnchorPane.topAnchor="0.0"
                                                                          AnchorPane.bottomAnchor="0.0"
                                                                          AnchorPane.leftAnchor="0.0"
                                                                          AnchorPane.rightAnchor="0.0"/>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </Tab>
                                                <Tab text="validation">
                                                    <content>
                                                        <AnchorPane AnchorPane.topAnchor="0.0"
                                                                    AnchorPane.bottomAnchor="0.0"
                                                                    AnchorPane.leftAnchor="0.0"
                                                                    AnchorPane.rightAnchor="0.0">
                                                            <children>
                                                                <TextArea fx:id="validationOut" disable="true"
                                                                          layoutY="-2.0" AnchorPane.topAnchor="0.0"
                                                                          AnchorPane.bottomAnchor="0.0"
                                                                          AnchorPane.leftAnchor="0.0"
                                                                          AnchorPane.rightAnchor="0.0"/>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </Tab>
                                            </tabs>
                                        </TabPane>
                                    </children>
                                </AnchorPane>
                            </items>
                        </SplitPane>
                    </children>

                </VBox>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
